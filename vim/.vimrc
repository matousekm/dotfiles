syntax on
filetype on
imap kj <esc>
set scrolloff=5
set rnu
"set number
set tabstop=4
map <space> /
set showmatch
set hlsearch
inoremap " ""<left>
inoremap ' ''<left>
inoremap ( ()<left>
inoremap [ []<left>
inoremap { {}<left>
inoremap ` ``<left>
inoremap < <><left>
set shiftwidth=4
set smarttab
set expandtab
set tabstop=8
set softtabstop=0
set autoindent
